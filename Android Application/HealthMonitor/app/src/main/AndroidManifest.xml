<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.healthmonitor">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HealthMonitor">
        <activity android:name=".EmailNotification"></activity>
        <activity android:name=".Bluetooth" />
        <activity
            android:name=".MainActivity2"
            android:label="@string/title_activity_main2" />
        <activity
            android:name=".my_nav"
            android:label="@string/title_activity_my_nav" />
        <activity
            android:name=".bottom_nav"
            android:label="@string/title_activity_bottom_nav" />
        <activity android:name=".ShowData" />
        <activity android:name=".my_data" />
        <activity android:name=".user_data" />
        <activity android:name=".User_registration" />
        <activity android:name=".Homepage" />
        <activity android:name=".Login" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme2">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.actions"
            android:resource="@xml/home_logout" />
    </application>

</manifest>